{
  "name": "openclaw-agentmail-listener",
  "version": "0.1.4",
  "description": "OpenClaw plugin: AgentMail listener â€” injects system events when emails arrive",
  "main": "dist/index.js",
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "README.md"
  ],
  "scripts": {
