{
  "name": "agentmail-listener",
  "version": "0.1.0",
  "description": "OpenClaw plugin: AgentMail listener \u2014 injects system events when emails arrive",
  "main": "dist/index.js",
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "README.md"
  ],
  "scripts": {
    "build": "esbuild index.ts --bundle --platform=node --target=node18 --outfile=dist/index.js --format=cjs --sourcemap --tree-shaking=true --external:openclaw --external:openclaw/*",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "openclaw",
    "openclaw-plugin",
    "agentmail",
    "email"
  ],
  "license": "MIT",
  "dependencies": {
    "agentmail": "^0.2.17"
  },
  "devDependencies": {
    "esbuild": "^0.24.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "openclaw": "*"
  }
}
