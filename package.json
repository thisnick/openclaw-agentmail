    "url": "https://github.com/thisnick/openclaw-agentmail/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "agentmail": "^0.2.17"
  },
  "devDependencies": {
    "esbuild": "^0.24.2",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "openclaw": "*"
  }
}
